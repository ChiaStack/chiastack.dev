{
  "$schema": "https://json.schemastore.org/package",
  "name": "@chiastack/services",
  "version": "0.0.1-beta.1",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .turbo node_modules dist",
    "lint": "eslint",
    "type:check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@chiastack/eslint": "workspace:*",
    "@chiastack/tsconfig": "workspace:*",
    "@chiastack/utils": "workspace:*",
    "@t3-oss/env-core": "catalog:",
    "eslint": "catalog:eslint",
    "ky": "catalog:",
    "tsdown": "catalog:",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "@chiastack/utils": "^0.0.1-beta.3",
    "@t3-oss/env-core": "^0.13.8",
    "ky": "^1.0.0",
    "zod": "^3.24.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@t3-oss/env-core": {
      "optional": true
    }
  },
  "exports": {
    "./package.json": "./package.json",
    "./captcha": {
      "import": "./dist/captcha/captcha.mjs",
      "types": "./dist/captcha/captcha.d.mts"
    },
    "./captcha/env": {
      "import": "./dist/captcha/env.mjs",
      "types": "./dist/captcha/env.d.mts"
    },
    "./captcha/env.client": {
      "import": "./dist/captcha/env.client.mjs",
      "types": "./dist/captcha/env.client.d.mts"
    },
    "./demo": {
      "import": "./dist/demo/demo.mjs",
      "types": "./dist/demo/demo.d.mts"
    },
    "./demo/env": {
      "import": "./dist/demo/env.mjs",
      "types": "./dist/demo/env.d.mts"
    }
  },
  "typesVersions": {
    "*": {
      "captcha": [
        "./dist/captcha/captcha.d.mts"
      ],
      "captcha/env": [
        "./dist/captcha/env.d.mts"
      ],
      "captcha/env.client": [
        "./dist/captcha/env.client.d.mts"
      ],
      "demo": [
        "./dist/demo/demo.d.mts"
      ],
      "demo/env": [
        "./dist/demo/env.d.mts"
      ]
    }
  }
}
