{
  "$schema": "https://json.schemastore.org/package",
  "name": "@chiastack/services",
  "version": "2.0.1",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "name": "Chia1104",
    "email": "yuyuchia7423@gmail.com",
    "url": "https://github.com/chia1104"
  },
  "keywords": [
    "services",
    "chiastack"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .turbo node_modules dist",
    "lint": "eslint",
    "type:check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@chiastack/eslint": "workspace:*",
    "@chiastack/tsconfig": "workspace:*",
    "@chiastack/utils": "workspace:*",
    "eslint": "catalog:eslint",
    "ky": "catalog:",
    "tsdown": "catalog:",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "@chiastack/utils": "^2.0.1"
  },
  "exports": {
    "./package.json": "./package.json",
    "./captcha": {
      "import": "./dist/captcha/captcha.mjs",
      "types": "./dist/captcha/captcha.d.mts"
    },
    "./demo": {
      "import": "./dist/demo/demo.mjs",
      "types": "./dist/demo/demo.d.mts"
    },
    "./demo/env": {
      "import": "./dist/demo/env.mjs",
      "types": "./dist/demo/env.d.mts"
    }
  },
  "typesVersions": {
    "*": {
      "captcha": [
        "./dist/captcha/captcha.d.mts"
      ],
      "demo": [
        "./dist/demo/demo.d.mts"
      ],
      "demo/env": [
        "./dist/demo/env.d.mts"
      ]
    }
  }
}
